{
    "cleanUrls": true,
    "rewrites" : [
        {"source": "/(.*)", "destination": "/"}
    ],
    "redirects": [
      {
        "source": "/:match*",
        "has": [
          {
            "type": "host",
            "value": "ashflow.info"
          }
        ],
        "destination": "https://www.ashflow.info/:match*",
        "statusCode": 308
      }
    ]
  }
  